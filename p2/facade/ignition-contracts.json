                    "reference": "3c921a1cdba35b68a7f0ccffc6dffc1995b18267"
                },
                "require": {
                    "php": "^7.3|^8.0"
                },
                "require-dev": {
                    "friendsofphp/php-cs-fixer": "^v2.15.8",
                    "phpunit/phpunit": "^9.3.11",
                    "vimeo/psalm": "^3.17.1"
                },
                "type": "library",
                "autoload": {
                    "psr-4": {
                        "Facade\\IgnitionContracts\\": "src"
                    }
                },
                "autoload-dev": {
                    "psr-4": {
                        "Facade\\IgnitionContracts\\Tests\\": "tests"
                    }
                },
                "scripts": {
                    "psalm": [
                        "vendor/bin/psalm"
                    ],
                    "format": [
