{
    "packages": {
        "facade/ignition-contracts": [
            {
                "name": "facade/ignition-contracts",
                "version": "1.0.2",
                "version_normalized": "1.0.2.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/facade/ignition-contracts.git",
