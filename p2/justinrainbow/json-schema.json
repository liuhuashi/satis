{
    "packages": {
        "justinrainbow/json-schema": [
            {
                "name": "justinrainbow/json-schema",
                "version": "5.2.10",
                "version_normalized": "5.2.10.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/justinrainbow/json-schema.git",
                    "reference": "2ba9c8c862ecd5510ed16c6340aa9f6eadb4f31b"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://satis.pythonschool.com/dist/2ba9c8c862ecd5510ed16c6340aa9f6eadb4f31b",
                    "reference": "2ba9c8c862ecd5510ed16c6340aa9f6eadb4f31b"
                },
                "require": {
                    "php": ">=5.3.3"
                },
                "require-dev": {
                    "friendsofphp/php-cs-fixer": "~2.2.20||~2.15.1",
                    "json-schema/json-schema-test-suite": "1.2.0",
                    "phpunit/phpunit": "^4.8.35"
                },
                "bin": [
                    "bin/validate-json"
                ],
                "type": "library",
                "extra": {
                    "branch-alias": {
                        "dev-master": "5.0.x-dev"
                    }
                },
                "autoload": {
                    "psr-4": {
                        "JsonSchema\\": "src/JsonSchema/"
                    }
                },
                "autoload-dev": {
                    "psr-4": {
                        "JsonSchema\\Tests\\": "tests/"
                    }
                },
                "scripts": {
                    "coverage": [
                        "phpunit --coverage-text"
                    ],
                    "style-check": [
                        "php-cs-fixer fix --dry-run --verbose --diff"
                    ],
                    "style-fix": [
                        "php-cs-fixer fix --verbose"
                    ],
                    "test": [
                        "phpunit"
                    ],
                    "testOnly": [
                        "phpunit --colors --filter"
                    ]
                },
                "license": [
                    "MIT"
                ],
                "authors": [
                    {
                        "name": "Bruno Prieto Reis",
                        "email": "bruno.p.reis@gmail.com"
                    },
                    {
                        "name": "Justin Rainbow",
                        "email": "justin.rainbow@gmail.com"
                    },
                    {
                        "name": "Igor Wiedler",
                        "email": "igor@wiedler.ch"
                    },
                    {
                        "name": "Robert Sch√∂nthal",
                        "email": "seroscho@googlemail.com"
                    }
                ],
                "description": "A library to validate a json schema.",
                "homepage": "https://github.com/justinrainbow/json-schema",
                "keywords": [
                    "json",
                    "schema"
                ]
            }
        ]
    }
}
